<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
	<class name="com.psl.entity.Status" table="STATUS_TABLE">
		<id name="statusID" column="STATUS_ID">
			<generator class="native"></generator>
		</id>
		<property name="trainingID" column="TRAINING_ID"></property>
		<property name="roomID" column="ROOM_ID"></property>
		<property name="date" column="DATE" type="date"></property>
		<property name="status" column="STATUS"></property>


	</class>

</hibernate-mapping>

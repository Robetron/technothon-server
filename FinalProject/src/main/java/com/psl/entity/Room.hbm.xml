<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.psl.entity.Room" table="ROOM_TABLE">
		<id name="roomID" column="ROOM_ID">
			<generator class="native"></generator>
		</id>
		<property name="roomName" column="ROOM_NAME" unique="true"></property>
		<property name="roomType" column="ROOM_TYPE"></property>
		<property name="capacity" column="CAPACITY"></property>
		<property name="location" column="LOCATION"></property>

	</class>

</hibernate-mapping>
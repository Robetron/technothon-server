<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="com.psl.entity.User" table="USER_TABLE">
		<id name="EmployeeID" column="EMPLOYEE_ID">
			<generator class="native"></generator>
		</id>
		<property name="EmployeeFirstName" column="EMPLOYEE_FNAME"></property>
		<property name="EmployeeLastName" column="EMPLOYEE_LNAME"></property>

		<property name="Username" column="USERNAME"></property>
		<property name="Password" column="PASSWORD"></property>
		<!-- <property name="Designation" column="DESIGNATION"></property> -->
		<property name="UserType" column="USERTYPE"></property>
	</class>

</hibernate-mapping>
